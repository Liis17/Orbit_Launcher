<fw:AcrylicWindow x:Class="OrbitLaunch.MainWindow"
        x:Name="StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OrbitLaunch"
        xmlns:fw="clr-namespace:SourceChord.FluentWPF;assembly=FluentWPF"
        mc:Ignorable="d"
        ResizeMode="CanMinimize"
        WindowStyle="None"
        AcrylicWindowStyle="None"
        WindowStartupLocation="CenterScreen"
        Title="OrbitLaunch" Height="100" Width="450">

    <Grid Background="#1A1A1A">

        <ProgressBar x:Name="StartupProgressBar" Maximum="2" Value="1" Margin="10,0,10,10" Height="19" VerticalAlignment="Bottom"/>
        <TextBlock x:Name="StartupTextBlock" Margin="10,0,10,34" TextWrapping="Wrap" Text="..." Foreground="White" FontFamily="Segoe UI Semibold" FontSize="25" TextAlignment="Center" Height="47" VerticalAlignment="Bottom"/>
        <!--Основной грид-->



    </Grid>
</fw:AcrylicWindow>
