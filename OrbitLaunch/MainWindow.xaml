<Window x:Class="OrbitLaunch.MainWindow"
        x:Name="StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OrbitLaunch"
        mc:Ignorable="d"
        ResizeMode="CanMinimize"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen"
        Title="OrbitLaunch" Height="150" Width="600">

    <Grid Background="#1A1A1A">

        <ProgressBar x:Name="StartupProgressBar" Maximum="100" Value="0" HorizontalAlignment="Left" Height="32" Margin="10,108,0,0" VerticalAlignment="Top" Width="580"/>
        <TextBlock x:Name="StartupTextBlock" HorizontalAlignment="Left" Margin="10,56,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="47" Width="580" Foreground="White" FontFamily="Segoe UI Semibold" FontSize="25" TextAlignment="Center"/>
        <!--Основной грид-->



    </Grid>
</Window>
